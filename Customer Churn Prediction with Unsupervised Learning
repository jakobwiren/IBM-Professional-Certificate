    "status_df = status_df.drop(['Count','Churn Category','Churn Reason', 'Churn Label', 'Customer Status', 'Churn Score'], axis = 1)\n",
    "status_df.columns\n",
    "services_df = services_df.drop(['Quarter', 'Count'], axis = 1)\n",
    "services_df.columns"
